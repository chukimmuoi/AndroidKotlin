<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.developers.chukimmuoi.androidkotlin.ui.custom.progress.CircleProgress
        android:id="@+id/progress_circle"
        android:layout_width="@dimen/item_circle_progress_80dp"
        android:layout_height="@dimen/item_circle_progress_80dp"
        android:layout_centerInParent="true"
        app:matProg_barColor="@color/colorProgressCircle"
        app:matProg_linearProgress="true"
        app:matProg_progressIndeterminate="true" />
</RelativeLayout>
